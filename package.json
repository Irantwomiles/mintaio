{
  "name": "mintaio",
  "version": "0.9.3",
  "private": true,
  "main": "public/electron.js",
  "build": {
    "productName": "MintAIO",
    "appId": "aio.mint",
    "win": {
      "icon": "build/ic.png"
    },
    "mac": {
      "target": "dmg",
      "icon":  "build/ic.png"
    }
  },
  "homepage": ".",
  "dependencies": {
    "@alch/alchemy-web3": "^1.1.8",
    "@popperjs/core": "^2.10.2",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "bootstrap": "^5.1.3",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "lowdb": "^1.0.0",
    "nedb": "^1.8.0",
    "node-machine-id": "^1.1.12",
    "node-sass": "^6.0.1",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "require-from-web": "^1.2.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "builder-win": "electron-builder --win",
    "builder-mac": "electron-builder --mac",
    "build-win": "npm run react-build && npm run builder-win",
    "build-mac": "npm run react-build && npm run builder-mac",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "electron": "^16.0.1",
    "electron-builder": "~22.10.5",
    "wait-on": "^6.0.0"
  }
}
